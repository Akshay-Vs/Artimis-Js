{
  "name": "@artimisjs/core",
  "version": "0.0.2",
  "description": "Artimis Core Library",
  "type": "module",
  "private": false,
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsup src --config ./.tsup.config.ts"
  },
  "keywords": [
    "artimisjs",
    "core",
    "react"
  ],
  "author": "Akshay-Vs",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@artimisjs/typescript-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  }
}
