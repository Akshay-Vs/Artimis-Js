  "version": "0.1.10",
  "type": "module",
  "author": "Akshay-Vs",
  "license": "MIT",
  "description": "Artimis.Js UI Library",
  "keywords": [
    "react",
    "ui",
    "artimis",
    "ecommerce"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/artimis-ui.umd.cjs",
  "module": "./dist/artimis-ui.js",
  "exports": {
    ".": {
      "import": "./dist/artimis-ui.js",
